{
  "apiVersion": 3,
  "name": "starter-kit/image",
  "title": "Image (SK)",
  "category": "starter-kit",
  "icon": "skb skb-image",
  "description": "Image",
  "keywords": [
    "Image",
    "Bootstrap"
  ],
  "textdomain": "",
  "styles": [],
  "supports": {},
  "example": {},
