    },
    "link": {
      "type": "object",
      "default": {
        "addLink": false,
        "targetBlank": false,
        "href": ""
      }
    },
    "fetchPriority": {
      "type": "string",
      "default": "auto"
    },
    "loadingLazy": {
      "type": "boolean",
      "default": true
    },
    "hidpi": {
      "type": "boolean",
      "default": true
